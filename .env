# .env

# AI Provider Configuration
REACT_APP_AI_PROVIDER=claude
REACT_APP_CLAUDE_API_KEY=your_claude_api_key
REACT_APP_CLAUDE_API_ENDPOINT=https://api.anthropic.com/v1/complete

REACT_APP_GEMINI_API_KEY=your_gemini_api_key

# Feature Flags
REACT_APP_ENABLE_REALTIME_FEEDBACK=true
REACT_APP_ENABLE_STROKE_ANALYSIS=true
REACT_APP_ENABLE_VOICE_GUIDANCE=false

# Analytics Configuration
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ANALYTICS_ID=your_analytics_id

# Cache Configuration
REACT_APP_CACHE_DURATION=3600
REACT_APP_MAX_CACHE_SIZE=100

# API Rate Limiting
REACT_APP_MAX_REQUESTS_PER_MINUTE=60
REACT_APP_THROTTLE_DELAY=1000

# Debug Configuration
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info